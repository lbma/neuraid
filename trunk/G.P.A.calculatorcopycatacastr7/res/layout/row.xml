<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/TableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    match_parent="" >

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/rowclassname"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Course"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/originalyellow" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Credits"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/originalyellow" />

        <Spinner
            android:id="@+id/rowspinner"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:entries="@array/course_credits" />

        <TextView
            android:id="@+id/rowgrade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Grade"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/originalyellow" />

        <Spinner
            android:id="@+id/rowspinnersecond"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:entries="@array/course_grade"
            tools:listitem="@android:layout/simple_spinner_dropdown_item" />

    </TableRow>

</TableLayout>